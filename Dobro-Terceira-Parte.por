programa {
  funcao inicio() {
    // Declaração de variáveis
    real n1, dobro, terc

    //Entrada de dados
    escreva("Insira um número: ")
    leia(n1)
    
    //Atribuição do cálculo para as variáveis de Dobro e a Terceira parte do valor de entrada
    dobro = n1 + n1
    terc = n1 / 3

    //Saída de Dados 
    escreva("O dobro do número inserido é: ", dobro, "\n")
    escreva("A terceira parte do número inserido é: ", terc, "\n")
  }
}
