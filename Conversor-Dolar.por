programa {
  funcao inicio() {
    //Declaração de variáveis
    real valor
    
    //Entrada do valor
    escreva("Insira o valor a ser convertido: ")
    leia(valor)
    
    //Cálculo das variáveis após a inserção para a conversão em dólar
    valor = valor * 5.21
    
    //Saída de dados (Resultado da conversão)
    escreva("Você tem ", valor, " em dólar")
  }
}
